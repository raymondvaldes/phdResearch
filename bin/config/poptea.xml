<poptea>
	<sweep>
		<minperDecade>50</minperDecare>
		<thermal_penetration>
			<start>.04</start>
			<end>4</end>
		</thermal_penetration>
		<frequency>			
			<start>0.02</start>
			<end>2000</end>		
		</frequency>			
	</sweep>
	
	<ParaEstSettings>
		<!--- N number of thermal parameters to be fitted-->
		<!-- ftol difference in error-->
		<!-- xtol difference in parameters-->
		<!-- beta_iter is the number of iterations to find beta2-->
		<!-- beta_tol is how close dz_sub is to dz_coat */-->
		<ftol>1e-08</ftol>
		<xtol>1e-08</xtol>
		<gtol>1e-08</gtol>
		<maxfev>100000</maxfev>
		<epsfcn>0.0001</epsfcn>
		<factor>10</factor>
		<mode>2</mode>
		<nprint>0</nprint>
	</ParaEstSettings>

	<unknownParameters>
		<unknown>
			<label>asub</label>
			<min>1e-0</min>
			<max>5</max>
			<initialGuess>2.1</initialGuess>
		</unknown>
		
		<unknown>
			<label>E1</label>
			<min>1</min>
			<max>200</max>
			<initialGuess>40</initialGuess>
		</unknown>		
		
		<unknown>
			<label>lambda</label>
			<min>0.1</min>
			<max>1.0</max>
			<initialGuess>0.5</initialGuess>
		</unknown>
		
		<unknown>
			<label>R1</label>
			<min>0.6</min>
			<max>1.0</max>
			<initialGuess>0.75</initialGuess>
		</unknown>
		
		<unknown>
			<label>gammaEff</label>
			<min>2e-0</min>
			<max>10</max>
			<initialGuess>3.7</initialGuess>
		</unknown>
		
	</unknownParameters>
	

	</poptea>
