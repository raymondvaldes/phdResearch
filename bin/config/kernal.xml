<kernal>
	<experimentalSetup>
		<Laser>
			<!--power - Watts-->
			<!--radius - m-->
			<power>30</power>
			<radius>20e-4</radius>
			<offset>0.95</offset>
			<amplitude>.05</amplitude>
		</Laser>

		<Detector>
			<!--radius  ** detector viewing radius-->
			<!--lambda  ** detector wavelength-->
			<radius>0.25e-3</radius>
			<lambda>5e-6</lambda>
		</Detector>		
	</experimentalSetup>
	
	<TBCsystem>
		<coating>
		  <conductivity>
				<offset>1.44</offset>
				<slope>0</slope>
			</conductivity>

			<thermalmass>
				<offset>2.1e6</offset>
				<slope>0</slope>
			</thermalmass>

			<lambda>0.57</lambda>
			<length>71.7e-6</length>
		</coating>

		<substrate>
			<conductivity>
				<offset>12.7</offset>
				<slope>0</slope>
			</conductivity>

			<thermalmass>
				<offset>3.44e6</offset>
				<slope>0</slope>
			</thermalmass>

			<lambda>0</lambda>
			<length>7098e-6</length>
		</substrate>
		
		<TemperatureScale>
			<!--Temperature scales must be in Kelvin-->
			<tolerance>1e-3</tolerance>
			<referance>300</referance>
			<base>273.15</base>
			<rear>0</rear>
		</TemperatureScale>			
		
		<RadiationProperties>
			<!--- Reflection; R0 at surface; R1 at interface-->
			<R0>0.2</R0>
			<R1>0.8</R1>
			<Emit1>42</Emit1>
		</RadiationProperties>	
		
		<radialDomain>0.25e-3</radialDomain>
	</TBCsystem>

	<ParaEstSettings>
		<!--- N number of thermal parameters to be fitted-->
		<!-- ftol difference in error-->
		<!-- xtol difference in parameters-->
		<!-- beta_iter is the number of iterations to find beta2-->
		<!-- beta_tol is how close dz_sub is to dz_coat */-->
		<ftol>1e-08</ftol>
		<xtol>1e-08</xtol>
		<gtol>1e-08</gtol>
		<maxfev>100000</maxfev>
		<epsfcn>0.0001</epsfcn>
		<factor>10</factor>
		<mode>0</mode>
		<nprint>0</nprint>
	</ParaEstSettings>
	
	<thermalModel>
		<heatModel>OneDimAnalytical</heatModel>
		<emissionModel>OneDimNonLin</emissionModel>
		<mesh>
			<M2>221</M2>
			<Rend>20</Rend>
			<Nend>80</Nend>
			<beta1>100</beta1>
			<split>0.5</split>
		</mesh>
	</thermalModel>
	
<unknownParameters>
		<unknown>
			<label>asub</label>
			<min>1e-0</min>
			<max>5</max>
			<initialGuess>2.1</initialGuess>
		</unknown>

		<unknown>
			<label>gammaEff</label>
			<min>2e-0</min>
			<max>10</max>
			<initialGuess>3.7</initialGuess>
		</unknown>

		<unknown>
			<label>E1</label>
			<min>1</min>
			<max>200</max>
			<initialGuess>40</initialGuess>
		</unknown>

		<unknown>
			<label>R1</label>
			<min>0.6</min>
			<max>1.0</max>
			<initialGuess>0.75</initialGuess>
		</unknown>

		<unknown>
			<label>lambda</label>
			<min>0.1</min>
			<max>1.0</max>
			<initialGuess>0.5</initialGuess>
		</unknown>
	</unknownParameters>
	
</kernal>
